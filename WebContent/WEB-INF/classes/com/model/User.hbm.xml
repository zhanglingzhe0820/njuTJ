<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.model.User" table="tb_student">
		<id name="id" column="id" type="int">
			<generator class="native"/>
		</id>
		<property name="qq" type="string" not-null="true" length="50">
			<column name="qq"/>
		</property>
		<property name="phone" type="string" not-null="true" length="50">
			<column name="phone"/>
		</property>
		<property name="number" type="string" not-null="true" length="50">
			<column name="number"/>
		</property>
		<property name="department" type="string" not-null="true" length="50">
			<column name="department"/>
		</property>
		<property name="name" type="string" not-null="true" length="50">
			<column name="name"/>
		</property>
		<property name="position" type="string" not-null="true" length="50">
			<column name="position"/>
		</property>
	</class>
</hibernate-mapping>